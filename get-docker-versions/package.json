{
  "name": "gh-actions",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "test": "npx jest",
    "package": "npm run test && npm run build && rm -rf tests src package.json",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "compare-versions": "^6.1.1"
  }
}
